@article{faquih_workflow_2020,
  title = {A {{Workflow}} for {{Missing Values Imputation}} of {{Untargeted Metabolomics Data}}},
  author = {Faquih, Tariq and {van Smeden}, Maarten and Luo, Jiao and {le Cessie}, Saskia and Kastenm{\"u}ller, Gabi and Krumsiek, Jan and Noordam, Raymond and {van Heemst}, Diana and Rosendaal, Frits R. and {van Hylckama Vlieg}, Astrid and {Willems van Dijk}, Ko and {Mook-Kanamori}, Dennis O.},
  year = {2020},
